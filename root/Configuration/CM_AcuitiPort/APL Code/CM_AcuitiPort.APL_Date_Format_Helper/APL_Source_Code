date strToDateCheck(string inDate, string dateFormat){
	date outDate;
	if(inDate != NULL_VALUE){
		if(!strToDate(outDate,inDate,dateFormat)){
			debug("Valid input string format: " +inDate);
         }
	}
	else{
        debug("strToDateCheck :Input is null");
	}
	return outDate;
}


string dateToStringCheck(date dEndWOrk, string dateFormat){
    string outString;
    if(dEndWOrk != NULL_VALUE)
    {
        dateToString(outString,dEndWOrk,dateFormat);     
    }    
    return outString;
